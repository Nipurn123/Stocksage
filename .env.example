# Example Environment Variables - Copy to .env and fill in your own values

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-key-here

# Database Configuration
DATABASE_URL="file:./prisma/dev.db"
# For production, use a real database URL:
# DATABASE_URL="postgresql://username:password@host:port/database"

# Gemini API Key
GEMINI_API_KEY="your-gemini-api-key-here"

# Stripe API Configuration
# Get your API keys from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# Zoho Inventory API Configuration
# Get your credentials from https://accounts.zoho.com/developerconsole
ZOHO_CLIENT_ID=your_zoho_client_id
ZOHO_CLIENT_SECRET=your_zoho_client_secret
ZOHO_REFRESH_TOKEN=your_zoho_refresh_token
ZOHO_ORGANIZATION_ID=your_zoho_organization_id

# API URL
NEXT_PUBLIC_API_URL=http://localhost:8000/api 